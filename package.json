    "angular": "^1.3.15",
    "angular-aria": "^1.3.15",
    "angular-mocks": "^1.3.15",
    "angular-route": "^1.3.15",
    "coffeelint": "^1.14.2",
    "compass": "^0.1.1",
    "connect": "^3.3.5",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
