{
  "name": "md-tutorial",
  "version": "0.0.0",
  "description": "Working towards a better markdown tutorial",
  "index": "scripts/test-server.js",
  "scripts": {
    "build": "webpack; grunt build",
    "coffeelint": "coffeelint ./Gruntfile.coffee",
    "compile": "npm run lint; npm run tsc;",
    "dev": "webpack-dev-server",
    "e2e": "",
    "jshint": "jshint ./spec/**/*.js ./scripts/*.js",
    "karma": "karma start ./etc/karma.conf.js --singleRun",
    "lint": "npm run coffeelint; npm run jshint; npm run tshint",
    "postinstall": "npm run typings; npm run build",
    "start": "node scripts/test-server.js",
    "test": "npm run karma",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings install",
    "tshint": "tslint -c ./etc/tslint.json ./src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bennett000/md-tutorial"
  },
  "keywords": [
    "markdown",
    "tutorial",
    "javascript"
