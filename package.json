    "grunt-protractor-runner": "^1.2.1",
    "grunt-text-replace": "^0.4.0",
    "html-webpack-plugin": "^2.9.0",
    "jasmine-core": "^2.2.0",
    "jshint": "^2.9.1",
    "karma": "^0.13.21",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
